#include <iostream>
using namespace std;
void reverse(int arr[],int n){
    int i=0,j=n-i-1;
    while(i<j){
        swap(arr[i],arr[j]);
        i++;
        j--;
    }
}
int main() {
    // Write C++ code here
    int arr[]={1,2,3,4};
    reverse(arr,4);
    for(int i=0;i<4;i++){
        cout<<arr[i];
    }
    return 0;
}
