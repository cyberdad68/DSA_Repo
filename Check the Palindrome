#include <bits/stdc++.h>
using namespace std;
int palindrome(int arr[],int n){
    int i=0,j=n-1;
    while(i<j){
        if(arr[i]==arr[j]){
            i++;
            j--;
        }
        else{
            return false;
        }
    }
    return true;
}
int main() {
    
    int arr[]={1,2,3,2,1};
    cout<<palindrome(arr,5);
    return 0;
}
